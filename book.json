{
    "title": "天地不仁，以万物为刍狗",
    "description": "多言数穷，不如守中",
    "author": "brekov.fg",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": "article",
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "anchor-navigation-ex",
        "-lunr",
        "-search",
        "-livereload",
        "highlight",
        "search-plus",
        "yahei",
        "theme-comscore",
        "multipart",
        "search-plus",
        "advanced-emoji",
        "include-codeblock",
        "ace",
        "emphasize",
        "katex",
        "splitter",
        "mermaid-gb3",
        "tbfed-pagefooter",
        "expandable-chapters-small",
        "sectionx",
        "-donate",
        "local-video",
        "favicon",
        "todo",
        "terminal",
        "alerts",
        "include-csv",
        "puml",
        "klipse",
        "-sharing",
        "sharing-plus",
        "graph",
        "chart",
        "sitemap-general"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "edit": true
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": true,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "facebook",
                "twitter",
                "weibo",
                "instapaper",
                "linkedin",
                "pocket",
                "stumbleupon",
                "qq",
                "qzone"
            ]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © saboop.com 2021",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "sitemap-general": {
            "prefix": "http://www.saboop.com"
        },
        "anchor-navigation-ex": {
            "isRewritePageTitle": false,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "sectionx": {
            "tag": "b"
        },
        "favicon": {
            "shortcut": "favicon.ico",
            "bookmark": "favicon.ico"
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        }
    }
}